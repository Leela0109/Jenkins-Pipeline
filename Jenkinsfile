  agent any
  stages {
    stage('build') {
      steps {
        echo 'I Want to Build'
      }
    }

    stage('Test') {
      steps {
        echo 'I want to Test'
      }
    }

    stage('Stage') {
      steps {
        echo 'I want to Stage'
      }
    }

    stage('Deploy') {
      steps {
        echo 'I Want to Deploy'
      }
    }

    stage('Operate') {
      steps {
        echo 'I Want to Operate'
      }
    }

  }
}